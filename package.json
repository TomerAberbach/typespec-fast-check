{
  "name": "typespec-fast-check",
  "version": "0.2.1",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomer@aberba.ch",
    "url": "https://tomeraberba.ch"
  },
  "description": "A TypeSpec emitter for fast-check arbitraries.",
  "keywords": [
    "arbitraries",
    "codegen",
    "fast-check",
    "typespec",
    "typespec-emitter"
  ],
  "homepage": "https://github.com/TomerAberbach/typespec-fast-check",
  "repository": "TomerAberbach/typespec-fast-check",
  "bugs": {
    "url": "https://github.com/TomerAberbach/typespec-fast-check/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/TomerAberbach"
  },
  "license": "MIT",
  "files": [
    "dist",
    "lib"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 22"
  },
  "exports": {
    ".": {
      "typespec": "./lib/main.tsp",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --fix .",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ENV=production vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "build": "tsdown"
  },
  "prettier": "@tomer/prettier-config",
  "dependencies": {
    "@alloy-js/core": "^0.3.0",
    "@alloy-js/typescript": "^0.3.0",
    "@rtsao/scc": "^1.1.0",
    "camelcase": "^8.0.0",
    "keyalesce": "^2.2.0",
    "lfi": "^4.1.1",
    "svkc": "^1.0.0"
  },
  "peerDependencies": {
    "@typespec/compiler": "^0.63.0"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.2.2",
    "@tomer/eslint-config": "^4.1.1",
    "@tomer/prettier-config": "^4.0.0",
    "@types/jsesc": "^3.0.3",
    "@types/node": "^24.3.0",
    "@types/pascalcase": "^1.0.3",
    "@typespec/compiler": "^0.63.0",
    "@typespec/http": "^0.63.0",
    "@typespec/openapi": "^0.63.0",
    "@typespec/openapi3": "^0.63.0",
    "@typespec/rest": "^0.63.0",
    "@typespec/versioning": "^0.63.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "fast-check": "^4.3.0",
    "jsesc": "github:TomerAberbach/jsesc#feat/typed-arrays",
    "module-from-string": "^3.3.1",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "serialize-error": "^12.0.0",
    "source-map-support": "^0.5.21",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
